# AgentResearch Environment Configuration

# Tavily API Configuration
# Get your API key from: https://tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here

# MongoDB Configuration
# For MongoDB Atlas, use: mongodb+srv://username:password@cluster.mongodb.net/database
# For local MongoDB, use: mongodb://localhost:27017
MONGODB_URI=mongodb://localhost:27017
DATABASE_NAME=agent_research

# Ollama Configuration
# Make sure Ollama is running locally: https://ollama.ai/
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Application Settings
DEBUG=False 